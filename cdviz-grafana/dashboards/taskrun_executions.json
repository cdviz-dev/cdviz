    },
    {
      "type": "stat",
      "transparent": false,
      "repeatDirection": "h",
      "title": "Total Duration",
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  COALESCE(SUM(extract('epoch' from (finished_at - started_at))), 0) AS total_duration\nFROM cdviz.taskrun\nWHERE\n  $__timeFilter(finished_at)\n  AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\n  AND finished_at IS NOT NULL",
          "refId": "",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "transparent": false,
      "repeatDirection": "h",
      "title": "Avg Runtime",
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  COALESCE(AVG(extract('epoch' from (finished_at - started_at))), 0) AS avg_runtime\nFROM cdviz.taskrun\nWHERE\n  $__timeFilter(finished_at)\n  AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\n  AND finished_at IS NOT NULL",
          "refId": "",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "blue"
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "transparent": false,
      "repeatDirection": "h",
      "title": "Failure Rate",
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT\n  COALESCE(\n    100.0 * COUNT(*) FILTER (WHERE outcome IN ('fail', 'failure', 'error')) / NULLIF(COUNT(*), 0),\n    0\n  ) AS failure_rate\nFROM cdviz.taskrun\nWHERE\n  $__timeFilter(finished_at)\n  AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\n  AND finished_at IS NOT NULL",
          "refId": "",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 25,
                "color": "red"
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "transparent": false,
      "repeatDirection": "h",
      "title": "Task Total Duration per Day",
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  time_bucket('1 day', finished_at) AS time,\n  SUM(extract('epoch' from (finished_at - started_at))) AS total_duration\nFROM cdviz.taskrun\nWHERE\n  $__timeFilter(finished_at)\n  AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\n  AND finished_at IS NOT NULL\nGROUP BY time\nORDER BY time",
          "refId": "",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "left",
            "axisWidth": 1,
            "barAlignment": 0,
            "barWidthFactor": 1,
            "drawStyle": "bars",
            "fillOpacity": 50,
            "gradientMode": "opacity",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 0,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "showValues": false,
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "sum",
            "max"
          ],
          "width": 300
        }
      }
    },
    {
      "type": "timeseries",
      "transparent": false,
      "repeatDirection": "h",
      "title": "Task Total Number of Run per Day",
      "datasource": {
        "type": "grafana-postgresql-datasource",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "format": "time_series",
          "rawSql": "SELECT\n  time_bucket('1 day', COALESCE(finished_at, started_at)) AS time,\n  CASE\n    WHEN finished_at IS NULL THEN 'cancelled'\n    WHEN outcome = 'success' THEN 'success'\n    WHEN outcome = 'pass' THEN 'pass'\n    WHEN outcome = 'fail' THEN 'fail'\n    WHEN outcome = 'failure' THEN 'fail'\n    WHEN outcome = 'error' THEN 'error'\n    WHEN outcome = 'skip' THEN 'skip'\n    WHEN outcome = 'skipped' THEN 'skip'\n    ELSE COALESCE(outcome, 'unknown')\n  END AS status,\n  COUNT(*) AS count\nFROM cdviz.taskrun\nWHERE\n  $__timeFilter(finished_at)\n  AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\nGROUP BY time, status\nORDER BY time, status",
          "refId": "",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "rawQuery": true
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "left",
            "axisWidth": 1,
            "barAlignment": 0,
            "barWidthFactor": 1,
            "drawStyle": "bars",
            "fillOpacity": 50,
            "gradientMode": "opacity",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 0,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "showValues": false,
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*(fail|error).*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*(cancel|skip).*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "semi-dark-orange"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*(pass|success|complet).*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*warn.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "yellow"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": ".*(unknown|other).*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "sum",
            "max"
          ],
          "width": 300
        }
      }
    },
    {
      "type": "row",
      "collapsed": true,
      "id": 0,
      "panels": [
        {
          "type": "cdviz-executiontable-panel",
          "transparent": false,
          "repeatDirection": "h",
          "title": "$selected_value - Summary",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${datasource}"
          },
          "targets": [
            {
              "format": "table",
              "rawSql": "WITH\n  -- Get last N executions per name with all details\n  execution_history AS (\n    SELECT\n      last_payload -> 'subject' -> 'content' ->> 'taskName' AS name,\n      subject_id,\n      outcome,\n      extract('epoch' from (finished_at - started_at)) AS run_duration,\n      \n      last_payload -> 'subject' -> 'content' ->> 'url' AS url,\n      started_at,\n      finished_at,\n      \n      row_number() OVER (PARTITION BY last_payload -> 'subject' -> 'content' ->> 'taskName' ORDER BY finished_at DESC) AS rn\n    FROM cdviz.taskrun\n    WHERE\n      $__timeFilter(finished_at)\n      AND last_payload -> 'subject' -> 'content' ->> 'taskName' = ANY(ARRAY[${selected_value:sqlstring}]::text[])\n      AND finished_at IS NOT NULL\n  ),\n\n  -- Aggregate statistics per name\n  aggregated_stats AS (\n    SELECT\n      name,\n      COALESCE(PERCENTILE_CONT(0.80) WITHIN GROUP (ORDER BY run_duration), 0) AS p80_duration,\n      COUNT(*) AS total_runs\n    FROM execution_history\n    GROUP BY name\n  ),\n\n  -- Create arrays of historical data for visualization\n  history_arrays AS (\n    SELECT\n      name,\n      COALESCE(array_agg(run_duration ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::numeric[]) AS run_duration_history,\n      \n      COALESCE(array_agg(outcome ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::text[]) AS outcome_history,\n      COALESCE(array_agg(subject_id ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::text[]) AS subject_id_history,\n      COALESCE(array_agg(url ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::text[]) AS url_history,\n      COALESCE(array_agg(started_at ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::timestamptz[]) AS started_times,\n      COALESCE(array_agg(finished_at ORDER BY finished_at ASC) FILTER (WHERE rn <= 20), ARRAY[]::timestamptz[]) AS completion_times\n      \n    FROM execution_history\n    WHERE rn <= 20\n    GROUP BY name\n  ),\n\n  -- Aggregate outcome counts across all runs per name\n  outcome_summary AS (\n    SELECT\n      name,\n      COUNT(*) FILTER (WHERE outcome IN ('pass', 'success')) AS passed,\n      COUNT(*) FILTER (WHERE outcome IN ('fail', 'failure', 'error')) AS failed,\n      COUNT(*) FILTER (WHERE outcome IN ('skip', 'skipped')) AS skipped\n    FROM execution_history\n    GROUP BY name\n  )\n\n-- Main query: 1 row per name\nSELECT\n  s.name AS \"Name\",\n  h.run_duration_history AS \"Run History (s)\",\n  h.outcome_history AS \"Outcome History\",\n  h.subject_id_history AS \"Run IDs\",\n  h.url_history AS \"URLs\",\n  h.started_times AS \"Started Times\",\n  h.completion_times AS \"Completion Times\",\n  \n  s.p80_duration AS \"P80 Duration (s)\",\n  s.total_runs AS \"Total Runs\",\n  COALESCE(o.passed, 0) AS \"Passed\",\n  COALESCE(o.failed, 0) AS \"Failed\",\n  COALESCE(o.skipped, 0) AS \"Skipped\"\nFROM aggregated_stats s\nLEFT JOIN history_arrays h ON s.name = h.name\nLEFT JOIN outcome_summary o ON s.name = o.name\nORDER BY s.name",
              "refId": "",
              "datasource": {
                "type": "grafana-postgresql-datasource",
                "uid": "${datasource}"
              },
              "editorMode": "code",
              "rawQuery": true
            }
          ],
          "gridPos": {
            "h": 16,
            "w": 24,
            "x": 0,
            "y": 12
          },
          "options": {
            "maxHistoryItems": 20,
            "showQueueHistory": false,
            "barHeight": 20,
            "barGap": 2
          }
        }
      ],
      "title": "Task: Latest $limit Executions",
      "gridPos": {
        "x": 0,
        "y": 9,
        "h": 1,
        "w": 24
      }
    }
  ],
  "version": 2026021419
}
